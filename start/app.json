{
  "expo": {
    "name": "TouchPoints",
    "slug": "start",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/loading5.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
